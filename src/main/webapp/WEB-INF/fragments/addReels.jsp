<form class="col-6 needs-validation" action="addReels" novalidate method="post">
    <div class="container">
        <table class="table caption-top">
            <thead class="table-success">
            <tr>
                <td class="text-center align-middle">
                    <label for="inputM" class="form-label">Model name</label>
                    <input type="text" class="form-control" name="modelName" id="inputM" required></td>
                <td class="text-center align-middle">
                    <label for="inputL" class="form-label">Body Material</label>
                    <input type="text" class="form-control" name="bodyMaterial" id="inputL" required></td>
                <td class="text-center align-middle">
                    <label for="inputP" class="form-label">Bearings</label>
                    <input type="text" class="form-control" name="bearings" id="inputP" required></td>
                <td class="text-center align-middle">
                    <label for="inputLur" class="form-label">Family Type</label>
                    <input type="text" class="form-control" name="familyType" id="inputLur" required></td>
                <td class="text-center align-middle">
                    <label for="inputLin" class="form-label">Handle</label>
                    <input type="text" class="form-control" name="handle" id="inputLin" required></td>
                <td class="text-center align-middle">
                    <label for="inputA" class="form-label">Spool Material</label>
                    <input type="text" class="form-control" name="spoolMaterial" id="inputA" required></td>
                <td class="text-center align-middle">
                    <label for="inputW" class="form-label">Models</label>
                    <input type="text" class="form-control" name="models" id="inputW" required></td>
                <td class="text-center align-middle">
                    <label for="inputS" class="form-label">Water Type</label>
                    <input type="text" class="form-control" name="waterType" id="inputS" required></td>

                <td class="text-center align-middle">
                    <div class="btn-group" role="group" aria-label="Basic example">
                        <button type="submit" class="btn btn-primary">Add reels</button>
                        <button type="reset" class="btn btn-secondary">Reset</button>
                    </div>
                </td>
            </tr>
            </thead>
        </table>
    </div>
</form>